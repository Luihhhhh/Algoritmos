Algoritmo ejercicio13
	Definir phombres, pmujeres Como Real;
	Escribir "Indicar cantidad de hombres";
	Leer Hombres;
	Escribir "Indicar cantidad de mujeres";
	Leer Mujeres;

	total<-Hombres+Mujeres ;
	phombres<-Hombres/Total;
	pmujeres<-Mujeres/Total;
	
	Escribir "Porcentaje de hombres: ", phombres;
	Escribir "Porcentaje de mujeres: ", pmujeres;
FinAlgoritmo
